        $("#chkHasDiscountCode").on("change", function () {
            //reset
            __(".discount-message").innerHTML = "";
            __("#txtDiscountCode").value = "";

            var section = $(".checkout-discount-section");

            if ($("#chkHasDiscountCode").is(':checked')) {
